{
  "type": "object",
  "properties": {
    "doc_type": {
      "type": "string",
      "pattern": "^driver$"
    },
    "version": {
      "type": "string",
      "format": "semver",
      "faker": "helpers.randomNumber"
    },
    "forename": {
      "type": "string",
      "minLength": 1,
      "faker": "name.firstName"
    },
    "surname": {
      "type": "string",
      "minLength": 1,
      "faker": "name.lastName"
    },
    "email": {
      "type": "string",
      "format": "email",
      "faker": "internet.email"
    },
    "phone": {
      "type": "string",
      "faker": "phone.phoneNumber"
    }
  },
  "required": [
    "doc_type",
    "version",
    "forename",
    "surname",
    "email",
    "phone"
  ]
}
